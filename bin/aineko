#!/usr/bin/ruby
#require 'conf'

module Aineko
  class Runner
    def inspect(pap)
      puts "inspecting #{pap[0]}"
      #load pap
    end

    def invoke(args)
      meth = args.shift
      begin
        send(meth, args)
      rescue NoMethodError
        puts "Method not supported: #{meth}"
      end
    end
  end
end

Aineko::Runner.new.invoke($*)

